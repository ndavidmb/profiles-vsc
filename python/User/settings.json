{
  "workbench.colorTheme": "Default Dark+",
  "tabnine.experimentalAutoImports": true,
  "editor.fontFamily": "'Cascadia Code', 'Cascadia Code PL Roman', 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 19,
  "editor.cursorBlinking": "solid",
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "onlySnippets",
  "editor.snippetSuggestions": "top",
  "editor.renderWhitespace": "none",
  "editor.tabSize": 8,
  "editor.lineNumbers": "relative",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#ffffff"
  },
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.surround": true,
  "vim.foldfix": true,
  "vim.cursorStylePerMode.normal": "block-outline",
  "vim.statusBarColors.surroundinputmode": "#007ACC",
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "g",
        "j"
      ],
      "commands": [
        "editor.action.marker.nextInFiles"
      ]
    },
    {
      "before": [
        "g",
        "k"
      ],
      "commands": [
        "editor.action.marker.prevInFiles"
      ]
    },
    {
      "before": [
        "<leader>",
        "l"
      ],
      "commands": [
        "workbench.action.splitEditorRight",
        "workbench.action.focusPreviousGroup",
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": [
        "<leader>",
        "n",
        "t"
      ],
      "commands": [
        "workbench.view.explorer"
      ]
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        "workbench.action.nextEditor"
      ]
    },
    {
      "before": [
        "J"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ]
    },
    {
      "before": [
        "L"
      ],
      "after": [
        "$"
      ]
    },
    {
      "before": [
        "<c-l>"
      ],
      "commands": [
        "workbench.action.focusNextGroup"
      ]
    },
    {
      "before": [
        "<c-h>"
      ],
      "commands": [
        "workbench.action.focusPreviousGroup"
      ]
    },
    {
      "before": [
        "<leader>",
        "p"
      ],
      "commands": [
        {
          "command": "workbench.action.quickOpen"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "q"
      ],
      "commands": [
        {
          "command": "workbench.action.closeActiveEditor"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "w"
      ],
      "commands": [
        {
          "command": "workbench.action.files.saveAll"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "a",
        "g"
      ],
      "commands": [
        {
          "command": "workbench.action.findInFiles"
        }
      ]
    },
    {
      "before": [
        "<c-n>"
      ],
      "commands": [
        "editor.action.insertCursorBelow"
      ]
    },
    {
      "before": [
        "<leader>",
        "s",
        "i"
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    },
    {
      "before": [
        "<leader>",
        "c",
        "i"
      ],
      "commands": [
        "editor.action.commentLine"
      ]
    },
    {
      "before": [
        "Y"
      ],
      "commands": [
        "editor.action.copyLinesDownAction"
      ]
    },
    {
      "before": [
        ","
      ],
      "after": [
        "A",
        ",",
        "<Esc>"
      ]
    },
    {
      "before": [
        "<leader>",
        "v"
      ],
      "after": [
        "I",
        "<Esc>",
        "l",
        "v",
        "$",
        "h"
      ]
    },
    {
      "before": [
        ";"
      ],
      "after": [
        "A",
        ";",
        "<Esc>"
      ]
    },
    {
      "before": [
        "<leader>",
        "s",
        "e"
      ],
      "after": [
        "i",
        "<Cr>",
        "<Esc>"
      ]
    },
    {
      "before": [
        "<leader>",
        "f"
      ],
      "after": [
        "leader",
        "leader",
        "2",
        "s"
      ]
    },
    {
      "before": [
        "<leader>",
        "a",
        "l"
      ],
      "commands": [
        "workbench.action.closeOtherEditors"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        "<leader>",
        "r"
      ],
      "commands": [
        "rest-client.request"
      ]
    },
    {
      "before": [
        "<leader>",
        "c",
        "i"
      ],
      "commands": [
        "editor.action.commentLine"
      ]
    },
    {
      "before": [
        "L"
      ],
      "after": [
        "%"
      ]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "<c-l>"
      ],
      "after": [
        "<Right>"
      ]
    },
    {
      "before": [
        "<c-h>"
      ],
      "after": [
        "<Left>"
      ]
    }
  ],
  "files.autoSave": "off",
  "git.autofetch": true,
  "terminal.external.windowsExec": "powershell.exe",
  "terminal.integrated.fontFamily": "'CaskaydiaCove NF', Consolas, 'Courier New', monospace",
  "terminal.integrated.fontSize": 18,
  "terminal.integrated.cursorStyle": "line",
  "emmet.showSuggestionsAsSnippets": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  }
}