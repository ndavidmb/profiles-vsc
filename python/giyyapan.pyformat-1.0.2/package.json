{
	"name": "pyformat",
	"displayName": "PyFormat",
	"description": "Providing code formatting and import organization using autopep8 and isort",
	"version": "1.0.2",
	"publisher": "giyyapan",
	"homepage": "https://github.com/giyyapan/vscode-pyformat",
	"repository": {
		"type": "git",
		"url": "https://github.com/giyyapan/vscode-pyformat"
	},
	"author": {
		"name": "giyyapan",
		"email": "giyyapan@gmail.com"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other",
		"Formatters"
	],
	"activationEvents": [
		"onLanguage:python",
		"onCommand:pyformat.sortImports"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "pyformat.sortImports",
				"title": "Sort Imports",
				"category": "PyFormat"
			}
		],
		"configuration": {
			"type": "object",
			"title": "PyFormat",
			"properties": {
				"pyformat.formatProvider": {
					"type": "string",
					"enum": [
						"autopep8",
						"black"
					],
					"description": "Which format provider should PyFormat use when formatting codes. Defaults to autopep8",
					"default": "autopep8",
					"scope": "resource"
				},
				"pyformat.autopep8Args": {
					"type": "array",
					"description": "Arguments passed to autopep8. Each argument is a separate item in the array.",
					"default": [
						"--max-line-length 120"
					],
					"items": {
						"type": "string"
					},
					"scope": "resource"
				},
				"pyformat.blackArgs": {
					"type": "array",
					"description": "Arguments passed to black. Each argument is a separate item in the array.",
					"default": [
						"--line-length 120"
					],
					"items": {
						"type": "string"
					},
					"scope": "resource"
				},
				"pyformat.isortArgs": {
					"type": "array",
					"description": "Arguments passed to isort. Each argument is a separate item in the array.",
					"default": [
						"-w 120",
						"-m 2"
					],
					"items": {
						"type": "string"
					},
					"scope": "resource"
				},
				"pyformat.enableImportOrganization": {
					"type": "boolean",
					"description": "Enable import organization functionality, defaults to true. You must restart vscode after change this",
					"default": true,
					"scope": "resource"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js",
		"publish": "vsce publish"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"@types/diff-match-patch": "^1.0.32",
		"@types/tmp": "^0.1.0",
		"diff-match-patch": "^1.0.4",
		"tmp": "^0.1.0"
	},
	"__metadata": {
		"id": "d4139deb-3b24-428d-aa95-257e155245e3",
		"publisherId": "7eff48e3-1db7-4720-833e-a4b64122c5ce",
		"publisherDisplayName": "Giyya Pan",
		"installedTimestamp": 1630466466430
	}
}